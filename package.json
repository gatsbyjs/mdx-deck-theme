{
  "name": "gatsby-mdx-theme",
  "version": "0.0.1",
  "main": "cjs/gatsby-mdx-theme.js",
  "module": "es/gatsby-mdx-theme.js",
  "author": "Dustin Schau <dustin@gatsbyjs.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "build": "rollup -c rollup.config.js && npm run babel",
    "babel": "babel src --out-dir es --source-maps",
    "babel:watch": "npm run babel -- --watch"
  },
  "files": [
    "cjs/*",
    "es/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0"
  },
  "peerDependencies": {
    "mdx-deck": "1.x.x",
    "prop-types": "15.x.x",
    "react": "16.x.x",
    "react-syntax-highlighter": "8.x.x",
    "styled-components": "3.x.x",
    "styled-system": "^3.1.4"
  },
  "dependencies": {}
}
